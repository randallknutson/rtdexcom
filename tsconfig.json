{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2017", "es2019.array"],
    "moduleResolution": "node",
    "module": "commonjs",
    "noEmit": false,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "strictNullChecks": false,
    "target": "es2017"
  },
  "include": ["./src/"]
}
